{
    "name": "cpliakas/acquia-sdk-php",
    "type": "library",
    "description": "An experimental PHP SDK for Acquia.",
    "keywords": ["acquia"],
    "homepage": "https://github.com/cpliakas/acquia-sdk-php",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "https://github.com/cpliakas/acquia-sdk-php/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "cpliakas/psolr": "0.2.*",
        "guzzle/service": "~3.0"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "pdepend/pdepend": "~1.0",
        "phploc/phploc": "~2.0",
        "phpunit/phpunit": "~3.0"
    },
    "replace": {
        "cpliakas/acquia-sdk-php-common": "self.version",
        "cpliakas/acquia-sdk-php-network": "self.version",
        "cpliakas/acquia-sdk-php-search": "self.version",
        "cpliakas/acquia-sdk-php-cloud-env": "self.version"
    },
    "autoload": {
        "psr-0": {
            "Acquia": "src/",
            "Acquia\\Test": "test/"
        }
    }
}
